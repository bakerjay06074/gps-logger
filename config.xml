<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.gps-logger-low-acc"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>GPS Logger low acc</name>

  <description>
      An example for using File System with PhoneGap.
  </description>
<!--<preference name="phonegap-version" value="cli-6.3.1" /> -->
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
   <plugin name="cordova-plugin-file" spec="~4.3.1" />
   <plugin name="cordova-plugin-geolocation" spec="~2.4.3" />
   <plugin name="cordova-plugin-vibration" spec="~2.1.3" />
</widget>
